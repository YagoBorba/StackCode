{
  "name": "@stackcode/core",
  "version": "1.0.3",
  "description": "Core logic for the StackCode tool.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc && cpx \"src/templates/**/*\" \"dist/templates\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": ["scaffolding", "templates", "automation", "release", "conventional-commits", "typescript"],
  "author": "Yago Borba",
  "license": "MIT",
  "homepage": "https://github.com/YagoBorba/StackCode#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/YagoBorba/StackCode.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/YagoBorba/StackCode/issues"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-core": "^9.0.0",
    "conventional-recommended-bump": "^11.2.0",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@types/conventional-changelog-core": "^8.0.0",
    "@types/semver": "^7.7.0",
    "vitest": "^3.2.4"
  }
